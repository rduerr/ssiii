DataProperty: hasFloeSize

    Annotations: 
        muo:preferredUnit <http://purl.oclc.org/NET/muo/ucum/unit/length/meter>
    
    
DataProperty: hasDevelopmentStageCode

    
DataProperty: hasIceThickness

