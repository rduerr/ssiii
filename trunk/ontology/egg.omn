        muo:preferredUnit <http://purl.org/nsidc/jcomm/egg#fractionalArea>
    
    
Class: 6_10ths

    EquivalentTo: 
        hasConcentrationCode value "6"^^<xsd:string>
    
    SubClassOf: 
        ssiii:IceWithConcentration
    
    
Class: wmo:Zone

    EquivalentTo: 
        hasComponent max 3 ssiii:ZoneComponent
    
    SubClassOf: 
        Zone
    
    
Class: muo:BaseUnit

    
Class: wmo:BigFloe

    EquivalentTo: 
        hasFloeSize only xsd:decimal[>= 500 , < 2000],
        hasIceFormCode value "5"^^xsd:string
    
    SubClassOf: 
        ssiii:IceWithForm
    
    
Class: wmo:FirstStageThinFirstYearIce

    SubClassOf: 
        hasDevelopmentStageCode value "8"^^<xsd:string>,
        wmo:ThinFirstYearIce
    
    
Class: ssiii:ZoneComponent

    EquivalentTo: 
        (hasDevelopmentStageCode exactly 1 xsd:string)
         and (hasConcentrationCode max 1 xsd:string)
         and (hasIceFormCode max 1 xsd:string)
    
    
Class: PredominantForm

    SubClassOf: 
        ssiii:IceWithForm
    
    
Class: 3_10ths

    EquivalentTo: 
        hasConcentrationCode value "3"^^<xsd:string>
    
