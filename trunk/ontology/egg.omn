    EquivalentTo: 
        hasConcentrationCode value "7"^^<xsd:string>
    
    SubClassOf: 
        ssiii:IceWithConcentration
    
    
Class: Zone

    EquivalentTo: 
        hasComponent min 1 ssiii:ZoneComponent
    
    SubClassOf: 
        hasComponent max 1 SecondaryForm,
        hasComponent max 1 PredominantForm,
        hasConcentrationCode exactly 1 xsd:string
    
    
Class: wmo:BrashIce

    SubClassOf: 
        ssiii:SmallIceCakeOrBrashIce
    
    
Class: ssiii:GrowlersIcebergsOrFloebergs

    EquivalentTo: 
        hasIceFormCode value "9"^^xsd:string,
